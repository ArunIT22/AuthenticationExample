@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

@if (User.Identity!.IsAuthenticated)
{

    <h3>Welcome, @User.Identity.Name</h3>

    <table class="table">
        @foreach (var item in User.Claims)
        {
            <tr>
                <td>@item.Type</td>
                <td>@item.Value</td>
            </tr>
        }
    </table>
}
