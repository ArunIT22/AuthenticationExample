@model IEnumerable<CountProductByBrandVM>

<h3>Product by Brand Name</h3>

<div class="row">
    <div class="col-md-5">
        <table class="table table-hover">
            <thead class="bg-light">
                <tr>
                    <th>Brand Name</th>
                    <th>Product Count</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.BrandName</td>
                        <td>@item.ProductCount</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>